<%- include('navbar') %>
<br>
    <div class="container jumbotron ">
        <h1 class="mb-4">Transfer Money</h1>
<form action="/customers/transfer" method="POST" autocomplete="off">
    <div class="form-group">
        <label for="title">Enter Account Number (Tranfer from) :</label>
        <input type="text"  required name="from" id="from" class="form-control" autcomplete='off' >
    
    </div>
    
    <div class="form-group">
        <label for="title">Enter Account Number (Transfer to) :</label>
        <input type="text"  required name="to" id="to" class="form-control" autcomplete='off' >
    
    </div>
    <div class="form-group">
        <label for="title">    Enter Amount to Transfer:</label>
        <input type="text"  required name="amount" id="amount" class="form-control" autcomplete='off' >
    
    </div>

    <button type="submit" class="btn btn-primary">Transfer Money</button>        </form>
    </div>
    <%- include('allCustomers') %>
    <%- include('footer') %> 


</body>
</html>